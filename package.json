{
	"name": "vite-plugin-small-css-merged-into-js",
	"version": "1.0.0",
	"description": "Merges css smaller than a specified byte into js",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"test": "vitest",
		"test:default": "vitest defaultConfiguration",
		"test:target": "vitest targetConfiguration",
		"prepublish": "npm run build"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tsconfig/svelte": "^5.0.4",
		"@types/node": "^22.7.9",
		"svelte": "^5.1.3",
		"typescript": "~5.6.3",
		"vite": "^5.4.10",
		"vitest": "^2.1.3"
	},
	"types": "index.d.ts",
	"author": "huangkongyan <yan_gcc@icloud.com>",
	"homepage": "https://github.com/KongYanHHHH/vite-plugin-small-css-merged-into-js",
	"bugs": {
		"url": "https://github.com/KongYanHHHH/vite-plugin-small-css-merged-into-js/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/KongYanHHHH/vite-plugin-small-css-merged-into-js.git"
	},
	"license": "MIT",
	"files": [
		"dist",
		"index.d.ts"
	],
	"keywords": [
		"vite-plugin",
		"small css merge to js",
		"small byte",
		"vite"
	],
	"main": "dist/index.umd.cjs",
	"module": "dist/index.js",
	"jsnext:main": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"default": "./dist/index.umd.cjs",
			"require": "./dist/index.umd.cjs",
			"types": "./index.d.ts"
		}
	}
}
